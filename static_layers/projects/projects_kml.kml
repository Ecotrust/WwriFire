		<SimpleData name="funder">Confederated Tribes and Bands of the Yakama Nation</SimpleData>
		<SimpleData name="prj_name">Condit Dam Riparian Restoration </SimpleData>
		<SimpleData name="website">http://www.yakamanation-nsn.gov/index.php</SimpleData>
		<SimpleData name="loc_notes"></SimpleData>
		<SimpleData name="y_coord">45.766667</SimpleData>
